---
layout: nil  
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Manuel Bonilla</title>
 <link href="http://manuelbonilla.github.com/atom.xml" rel="self"/>
 <link href="http://manuelbonilla.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://manuelbonilla.github.com</id>
 <author>
   <name>Manuel Bonilla</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://manuelbonilla.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://manuelbonilla.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
